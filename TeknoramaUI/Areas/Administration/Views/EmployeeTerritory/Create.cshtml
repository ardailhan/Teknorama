@model TeknoramaUI.Areas.Administration.Models.EmployeeTerritoryModel.EmployeeTerritoryCreateRequestModel
@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h2>New Employee Territory Page</h2>
<br />
<form class="form-group" method="post">
    @Html.LabelFor(x=>x.EmployeeId)
    @Html.TextBoxFor(x=>x.EmployeeId, new{@class = "form-control"})
    <br />
    @Html.LabelFor(x=>x.TerritoryId)
    @Html.TextBoxFor(x=>x.TerritoryId, new{@class = "form-control"})
    <br />
    <button class="btn btn-info">Save</button>
</form>