@using TeknoramaUI.Areas.Administration.Models.ShipperModel;
@model List<ShipperListResponseModel>
@{
    ViewData["Title"] = "List";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<table class="table table-bordered" style="margin-top:20px">
    <tr>
        <th>Id</th>
        <th>Company Name</th>
        <th>Phone Number</th>
    </tr>
    @if (Model.Count > 0)
    {
        @foreach (var item in Model)
        {
            <tr>
                <th>@item.Id</th>
                <th>@item.CompanyName</th>
                <th>@item.PhoneNo</th>
            </tr>
        }
    }
    else
    {
        <tr>
            <td colspan="2" class="text-danger">There is no any data..!</td>
        </tr>
    }
</table>